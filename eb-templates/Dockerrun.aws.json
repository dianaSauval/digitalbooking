{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "front-app",
            "image": "$AWS_ECR_REGISTRY/$REPOSITORY_NAME:$TEAM-front-$CI_PIPELINE_IID",
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ]
        },
        {
            "name": "back-app",
            "image": "$AWS_ECR_REGISTRY/$REPOSITORY_NAME:$TEAM-back-$CI_PIPELINE_IID",
            "links": ["front-app"],
            "portMappings": [
                {
                    "hostPort": 8080,
                    "containerPort": 8080
                }
            ]
        }
    ],
    "authentication": {
        "bucket": "$AWS_S3_BUCKET",
        "key": "remo/digitalbooking/dev/auth.json"
    }
}